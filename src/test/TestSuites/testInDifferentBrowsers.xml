<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="DifferentBrowsers" >

	<test name="SearchTestInChrome" parallel="tests" thread-count="10">
		<parameter name="browser" value="chrome" />
		<parameter name="platform" value="WINDOWS" />
		<classes>
			<class name="GridDockerRun.SearchTest" />
		</classes>
	</test>
	<test name="SearchTestInFirefox">
		<parameter name="browser" value="firefox" />
		<parameter name="platform" value="WINDOWS" />
		<classes>
			<class name="GridDockerRun.SearchTest" />
		</classes>
	</test>
</suite>